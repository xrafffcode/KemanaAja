<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Transparent" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="editText_style">
        <item name="android:padding">16dp</item>
        <item name="android:textSize">14dp</item>
        <item name="android:fontFamily">@font/nunito</item>
        <item name="android:textColor">#111B21</item>
        <item name="android:background">@drawable/form</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColorHint">#BCBEC4</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:singleLine">true</item>

    </style>


</resources>